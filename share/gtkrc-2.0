# $Id: gtkrc-2.0,v 1.2 2007/12/26 19:19:59 friedman Exp $

# This file should be installed as ~/.gtkrc-2.0

# Gnome applications may ignore this file in preference to the gconf
# registry.  To specify similar settings, run the following commands:
#     gconftool-2 --set /desktop/gnome/interface/gtk_theme     Clearlooks --type string
#     gconftool-2 --set /desktop/gnome/interface/gtk_key_theme Emacs      --type string
# Or see $sinit/bin/gnome-init-prefs.

gtk-theme-name     = "Clearlooks-Curve"

# This loads /usr/share/themes/Emacs/gtk-2.0-key/gtkrc
gtk-key-theme-name = "Emacs"

